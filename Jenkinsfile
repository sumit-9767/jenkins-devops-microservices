//Declarative Method
pipeline{
	agent any
	stages{
		stage('Build'){
			steps{
				echo "Build"
			}

		}
		stage('Test'){
			steps{

				echo "test"

			}

		}
		stage('Intergration Test'){
			steps{
				echo "Intergration Test"
			}

		}
	}
}








